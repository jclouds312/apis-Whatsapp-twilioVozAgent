
# WhatsApp Web.js Bot Configuration
# No se requiere configuración adicional, usa autenticación local

# WhatsApp Business Cloud API Configuration
WA_PHONE_NUMBER_ID=your_phone_number_id_here
WA_ACCESS_TOKEN=your_access_token_here
WA_WEBHOOK_VERIFY_TOKEN=your_webhook_verify_token_here

# Twilio Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number

# Facebook SDK Configuration
FACEBOOK_APP_ID=your_facebook_app_id
FACEBOOK_APP_SECRET=your_facebook_app_secret
FACEBOOK_ACCESS_TOKEN=your_facebook_access_token

# Server Configuration
PORT=5000
NODE_ENV=development
FRONTEND_URL=http://localhost:5000

# Database Configuration (si se requiere en el futuro)
# DATABASE_URL=your_database_url_here
